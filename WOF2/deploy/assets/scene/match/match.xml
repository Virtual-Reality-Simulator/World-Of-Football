			<quad texture="../../tex/quad.png" scale="1.0" alpha="0.99" blendType="alpha" texFilter="aniso" />
		</shapes>
		
		<ballPitchMarker scale="1.0" texture="../../tex/ball_drop.png" heightOffset="0.015" alpha="0.99" texFilter="aniso" blendType="alpha" />
		
		<pitchInfo width="90.0" length="120.0" penaltyBoxWidth="40" penaltyBoxLength="17" goalBoxWidth="18.0" goalBoxLength="5.0" goalWidth="7.32" goalHeight="2.44" />
		
		<footballerSwitching 
		

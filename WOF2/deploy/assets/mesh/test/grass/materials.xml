<WE>

	<physicalProxy type="mesh" mapping="merge" material="[physMaterial]" parameters="physMaterial" >
		
		<simplification enabled="false" mode="planar" planarAngle="2.5" loopMode="iterate" stepLimit="-1" considerAesthetics="true" useIslands="true" />
			
	</physicalProxy>

	<materials>
		
		<material_0 type="material" name="grass/grass" poolId="auto">
			<lighting enable="true" />
			<ambient value="1" />
			<diffuse value="1" />
			<emmisive value="0,0,0" />
			<specular enable="false" value="0.9" power="50" />
			<texture minFilter="linear" magFilter="linear" mipFilter="point" stage="0" >
				<source file="grass.png" mipFilter="default" genMipmaps="true" poolId="auto"/>
				<constraints>
					<minQuality minFilter="point" magFilter="linear" mipFilter="point" />
					<maxQuality minFilter="point" magFilter="linear" mipFilter="point" />
				</constraints>
			</texture>
		</material_0>
		
		<material_1 type="material" name="grass/grass_pattern" poolId="auto">
			<lighting enable="true" />
			<depthBias value="-0.0005" />
			<alpha enable="true" />
			<ambient value="1" />
			<diffuse value="1" />
			<emmisive value="0,0,0" />
			<specular enable="false" value="0.9" power="50" />
			<texture minFilter="linear" magFilter="linear" mipFilter="point" stage="0" >
				<source file="grass_pattern_00.png" mipFilter="default" genMipmaps="true" poolId="auto"/>
				<constraints>
					<minQuality minFilter="point" magFilter="linear" mipFilter="point" />
					<maxQuality minFilter="point" magFilter="linear" mipFilter="point" />
				</constraints>
			</texture>
		</material_1>
		
	</materials>
	
</WE>
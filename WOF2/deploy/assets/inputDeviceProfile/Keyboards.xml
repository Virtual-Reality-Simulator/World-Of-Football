<WE>
	
	<profile gameName="Keyboard Arrows" >
		
		<controller type="Keyboard" mark="false" />
		
		<map from="Num1" to="Pass" />
		<map from="Num2" to="ShotHigh" />
		<map from="Num3" to="ShotExtra" />
		<map from="Num4" to="ShotLow" />
		<map from="Num5" to="Modifier" />
		<map from="Num6" to="Modifier2" />
		<mapToAxis2D fromNeg="Down" fromPos="Up" to="FwdAxis"/>
		<mapToAxis2D fromNeg="Left" fromPos="Right" to="RightAxis"/>
		
		<mapAnd controller="@self" from1="Modifier" from2="Modifier2" to="Switch" />
		
	</profile>
	
	<profile gameName="Keyboard WASD" >
		
		<controller type="Keyboard" mark="false" />
		
		<map from="u" to="Pass" />
		<map from="i" to="ShotHigh" />
		<map from="o" to="ShotExtra" />
		<map from="k" to="ShotLow" />
		<map from="j" to="Modifier" />
		<map from="l" to="Modifier2" />
		<mapToAxis2D fromNeg="s" fromPos="w" to="FwdAxis"/>
		<mapToAxis2D fromNeg="a" fromPos="d" to="RightAxis"/>
		
		<mapAnd controller="@self" from1="Modifier" from2="Modifier2" to="Switch" />
		
	</profile>
	
</WE>